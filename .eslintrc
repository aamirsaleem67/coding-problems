{
    "root": true,
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "jest/globals": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:import/recommended"],
    "rules": {
        "no-console": "off",
        "prettier/prettier": ["error", {}, { "usePrettierrc": true }]
    }
}
